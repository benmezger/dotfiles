XF86KbdBrightnessDown
    /usr/local/bin/kbdlight down 5
XF86KbdBrightnessUp
    /usr/local/bin/kbdlight up 5

XF86MonBrightnessUp
    /usr/bin/light -A 5
XF86MonBrightnessDown 
    /usr/bin/light -U 5

XF86AudioRaiseVolume
	/usr/bin/amixer -q set Master 2dB+ unmute
XF86AudioLowerVolume
	/usr/bin/amixer -q set Master 2dB- unmute
XF86AudioMute
	/usr/bin/amixer -q set Master toggle
